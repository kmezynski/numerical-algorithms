% Cholesky factorization
%
% Input:
%   A - matrix
% Output:
%   symetric - bool: 1 - matrix is symetric, 0 - matrix is not symetric


function [L] = isPosDef(A)
    [rows, ~] = size(A);
    L = zeros(rows);
    
    
    % perform Crout's algorithm
    for k = 1:rows
        L(k,k) = sqrt(A(k,k) - sum (L(k, 1:(k-1)).^2));
        for j=(k+1):rows
            L(j,k) = (1/L(k,k)) * (A(j,k) - sum((L(j, 1:k-1).*L(k, 1:k-1))));
        end
    end
end